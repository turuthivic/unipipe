<div id="page-wrapper">
<!-- Header -->
<header id="header">
  <h1><%= link_to "Unipipe", root_path %></h1>
  <nav id="nav">
    <ul>
      <li><%= link_to "Home", root_path %></li>
      <li>
        <%= link_to "Chatrooms", chatrooms_path %>
      </li>
      <li>
        <%= link_to "Contact Us", welcome_contact_path, :class => 'icon' %>
      </li>
      <% if current_user %>
      <li><%= link_to "Log out", logout_path, method: "delete" %></li>
      <% else %>
      <li><%= link_to "Sign Up", signup_path, :class => 'button' %>  | </li>
      <li><%= link_to "Log in", login_path, :class => 'button' %></li>
      <% end %>
    </ul>
  </nav>
</header>
</div>